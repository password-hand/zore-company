page {
  background: #f5f5f5;
}

/*没有申报记录  */

.recode-box {
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 50%;
}

.recode-box image {
  width: 120rpx;
  height: 120rpx;
}

.text1 {
  font-size: 30rpx;
  color: #999;
}

.text2 {
  font-size: 26rpx;
  color: #bbb;
}

.addContent {
  padding-bottom: 170rpx;
}

.my-company-tit {
  padding: 0 34rpx;
  margin: 30rpx 0;
  background: white;
  height: 90rpx;
}

.shibei {
  width: 150rpx;
}

.shibei text {
  display: block;
  font-size: 28rpx;
  color: #666;
}

.query {
  width: 100rpx;
  height: 50rpx;
  font-size: 24rpx;
  background: #4e8cee;
  line-height: 50rpx;
  border-radius: 6rpx;
  text-align: center;
  color: white;
}

.image-icon {
  width: 40rpx;
  height: 40rpx;
}

.image-icons {
  height: 50rpx;
  text-align: right;
}

.image-sty {
  line-height: 30rpx;
  position: relative;
}

.image-sty image {
  position: absolute;
  right: 10rpx;
  top: 15rpx;
}

.placeholder {
  font-size: 24rpx;
  color: #999;
}

.my-company-body {
  background: white;
}

.my-company-body .part {
  height: 90rpx;
  padding: 0 34rpx;
  border-bottom: 2rpx solid #f5f5f5;
}

.part text {
  font-size: 28rpx;
  color: #666;
  width: 150rpx;
}

textarea {
  width: 325rpx;
  height: 70rpx;
  font-size: 28rpx;
  color: #333;
  overflow: hidden;
  margin-top: 15rpx;
}

.my-company-foot {
  margin-top: 23rpx;
}

.show-style {
  width: 440rpx;
  font-size: 28rpx;
  color: #333;
}

input {
  border: 0;
  font-size: 28rpx;
  color: #333;
}

.start {
  justify-content: flex-start;
}

.img-size {
  width: 24rpx;
  height: 14rpx;
  text-align: center;
}

.ver {
  width: 140rpx;
  height: 50rpx;
  border-radius: 6rpx;
  text-align: center;
  line-height: 50rpx;
  font-size: 24rpx;
}

.pic-ver {
  background: #f5f5f5;
  color: rgb(102, 102, 102);
  text-align: center;
  line-height: 50rpx;
}

.phone-ver {
  border: 2rpx solid rgb(78, 140, 238);
  color: rgb(78, 140, 238);
}

.picker {
  font-size: 28rpx;
  color: #333;
}

.btncom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 160rpx;
  background: #f5f5f5;
}

.add-list {
  margin: 30rpx 20rpx;
  padding: 20rpx 30rpx 20rpx 20rpx;
  background: white;
  height: 216rpx;
  border-radius: 6rpx;
  display: flex;
  justify-content: space-around;
  align-items: flex-start;
  flex-direction: column;
}
