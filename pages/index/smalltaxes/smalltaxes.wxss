.header {
  border-top: 4rpx solid #f5f5f5;
  border-bottom: 2rpx solid #f5f5f5;
  padding: 0 34rpx;
  height: 70rpx;
}

.header view, .header text {
  font-size: 28rpx;
  color: #999;
}

.main {
  padding: 0 34rpx;
  border-bottom: 2rpx solid #f5f5f5;
  height: 130rpx;
}

.main-left image {
  width: 40rpx;
  height: 40rpx;
}

.main-right {
  flex-direction: column;
}

.main-right text {
  font-size: 26rpx;
  color: #999;
}

.taxse-con {
  margin-left: 40rpx;
}

.taxse-con .top {
  font-size: 30rpx;
  color: #333;
}

.taxse-con .bottom {
  font-size: 26rpx;
  color: #999;
  margin-top: 16rpx;
}

.btntaxe {
  position: fixed;
  bottom: 0;
  left: 0;
  background: white;
  height: 147rpx;
  width: 100%;
}

.main-right text.active {
  color: #4e8cee;
}


/*没有申报记录  */

.recode-box {
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  display: flex;
  flex-direction: column;
  justify-content: space-around;
  align-items: center;
  /* //margin: 600rpx 0 32rpx; */
}

.recode-box image {
  width: 120rpx;
  height: 120rpx;
}

.text1 {
  font-size: 30rpx;
  color: #999;
}

.text2 {
  font-size: 26rpx;
  color: #bbb;
}
.scroll:last-child {
  margin-bottom: 180rpx;
} 